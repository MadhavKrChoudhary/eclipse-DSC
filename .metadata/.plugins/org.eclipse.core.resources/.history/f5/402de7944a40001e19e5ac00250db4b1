// class -> Caps latter
//       e.g ->  MyDisplay 
// methods -> small ltr for first word, caps for the next word
//     e.g ->  myDisplay()

class A{
	int a=10;
	int b=20;
	public void add() {
		int sum=a+b;
		System.out.println("sum = "+ sum);
	}
}
public class FirstOops {
	public static void main(String args[]) {
		
		//Syntax for declaring an object
		// ClassName obj; -> Reference of object
		// obj = new ClassName() 
		
		//ClassName obj = new ClassName();
		
		A objA = new A();
		System.out.println(objA.a);
		System.out.println();
		objA.add();
	}
}

